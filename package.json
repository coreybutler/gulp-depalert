{
  "name": "gulp-depalert",
  "version": "0.0.1",
  "description": "Identify outdated dependencies in projects via a console warning/report.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tape test/**/*.js | tap-spec && snazzy --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coreybutler/gulp-depalert.git"
  },
  "keywords": [
    "gulpplugin",
    "dependencies",
    "dependency",
    "check",
    "update",
    "alert",
    "electron"
  ],
  "author": "Corey Butler",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/coreybutler/gulp-depalert/issues"
  },
  "homepage": "https://github.com/coreybutler/gulp-depalert#readme",
  "devDependencies": {
    "snazzy": "^4.0.1",
    "standard": "6.0.8",
    "standard-tap": "^1.0.1",
    "tap-spec": "^4.1.1",
    "tape": "4.6.0"
  },
  "standard": {
    "ignore": [
      "_*",
      "_**/*",
      "gulpfile.js"
    ],
    "globals": [
      "window",
      "EventEmitter",
      "CustomException",
      "DOMParser",
      "XMLHttpRequest",
      "FormData",
      "btoa",
      "localStorage",
      "Network"
    ]
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "gulp-util": "^3.0.7",
    "semver": "^5.3.0"
  }
}
